{
  "name": "LazyMansMeal.github.io",
  "version": "1.0.0",
  "description": "Meal Planning Made Easy",
  "scripts": {
    "start": "react-scripts start",
    "tailwind": "postcss src/assets/css/tailwind.css -o public/tailwind.css",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LazyMansMeal/LazyMansMeal.github.io.git"
  },
  "keywords": [
    "meal planning",
    "food",
    "groceries",
    "shopping",
    "pickup",
    "budget",
    "easy"
  ],
  "bugs": {
    "url": "https://github.com/LazyMansMeal/LazyMansMeal.github.io/issues"
  },
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "postcss-cli": "^7.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "tailwindcss": "^1.6.2"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
